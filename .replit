run = "npm run dev & poetry run uvicorn main:app --host 0.0.0.0 --port 8000 & wait"
entrypoint = "main.py"
modules = ["postgresql-16", "python-3.9"]

[env]
PYTHON_VERSION = "3.9"
POETRY_VIRTUALENVS_IN_PROJECT = "true"

[nix]
channel = "stable-23_11"

[deployment]
run = ["sh", "-c", "npm run start"]
deploymentTarget = "cloudrun"
